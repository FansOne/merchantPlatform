page {
  background-color: #fff;
}
.Mask {
  position: absolute;
  top: 0;
  left: 0;
  z-index: 9;
  background-color: rgba(0, 0, 0, 0.6);
  width: 100%;
}
.sharePopupBox {
  box-sizing: border-box;
  padding: 0 30rpx;
  position: absolute;
  background-image: url('http://www.qumatou.com.cn/zheng/xcximage/shareBg_.png');
  background-size: 100%;
  z-index: 10;
  width: 524rpx;
  height: 860rpx;
  left: 0;
  right: 0;
  margin: auto;
  top: 95rpx;
}
.sharePopupBox .sharePopupBox_top {
  margin-top: 265rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.sharePopupBox .sharePopupBox_top view:nth-child(1) {
  font-size: 30rpx;
  color: #000;
  font-weight: bold;
}
.sharePopupBox .sharePopupBox_top view:nth-child(2) {
  font-size: 74rpx;
  color: #000;
}
.sharePopupBox .sharePopupBox_top view:nth-child(2) text {
  font-size: 50rpx;
}
.sharePopupBox .sharePopupBox_bottom {
  margin-top: 50rpx;
  position: relative;
}
.sharePopupBox .sharePopupBox_bottom view:nth-child(1) {
  font-size: 27rpx;
  color: #000;
}
.sharePopupBox .sharePopupBox_bottom .sharePopupBox_bottomSelectBtn {
  padding-top: 25rpx;
  display: flex;
  padding-bottom: 25rpx;
}
.sharePopupBox .sharePopupBox_bottom .sharePopupBox_bottomSelectBtn .sharePopupBox_bottomSelectBtnItem {
  flex: .5;
  display: flex;
  align-items: center;
  justify-content: center;
}
.sharePopupBox .sharePopupBox_bottom .sharePopupBox_bottomSelectBtn .sharePopupBox_bottomSelectBtnItem icon {
  margin: 15rpx;
}
.sharePopupBox .sharePopupBox_bottom .sharePopupBox_bottomSelectBtn .sharePopupBox_bottomSelectBtnItem view {
  font-size: 28rpx;
}
.sharePopupBox .sharePopupBox_bottom .cardNumber {
  box-sizing: border-box;
  padding-left: 25rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 27rpx;
  width: 380rpx;
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
  border: 1rpx solid #f1f1f1;
}
.sharePopupBox .sharePopupBox_bottom button {
  width: 350rpx;
  height: 90rpx;
  line-height: 90rpx;
  background-color: #ffd270;
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
  top: 270rpx;
}
.userMesseBox {
  box-sizing: border-box;
  padding: 0 20rpx;
  background-color: #ffd270;
  height: 380rpx;
}
.userMesseBox .userMesseContant {
  transform: translateY(35rpx);
  background-image: url('http://www.qumatou.com.cn/zheng/xcximage/cardbg.png');
  height: 250rpx;
  background-repeat: no-repeat;
  background-size: 100%;
  border-radius: 20rpx;
  display: flex;
}
.userMesseBox .userMesseContant .userMesseContant_left,
.userMesseBox .userMesseContant .userMesseContant_right {
  position: absolute;
  left: 0;
  height: 250rpx;
  box-sizing: border-box;
  padding: 20rpx;
}
.userMesseBox .userMesseContant .userMesseContant_left {
  display: flex;
}
.userMesseBox .userMesseContant .userMesseContant_left view {
  color: #fff;
  font-size: 32rpx;
  padding-left: 20rpx;
  height: 32rpx;
  line-height: 32rpx;
  margin-top: 50rpx;
}
.userMesseBox .userMesseContant .userMesseContant_left image {
  margin-left: 30rpx;
  margin-top: 20rpx;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  border: 1px solid #fff;
}
.userMesseBox .userMesseContant .userMesseContant_right {
  display: flex;
  flex-direction: column;
  align-items: flex-end;
  position: absolute;
  right: 0;
}
.userMesseBox .userMesseContant .userMesseContant_right .userMesseContant_rightShare {
  color: #fff;
  width: 125rpx;
  padding: 0;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  margin: 0;
  background-color: rgba(0, 0, 0, 0);
}
.userMesseBox .userMesseContant .userMesseContant_right .userMesseContant_rightShare view {
  font-size: 25rpx;
  margin-right: 10rpx;
  color: #fff;
}
.userMesseBox .userMesseContant .userMesseContant_right .userMesseContant_rightShare image {
  width: 35rpx;
  height: 37rpx;
}
.userMesseBox .userMesseContant .userMesseContant_right .userMesseContant_rightMoney {
  margin-top: 20rpx;
  font-size: 30rpx;
  color: #fff;
}
.userMesseBox .userMesseContant .userMesseContant_right .userMesseContant_rightMoney text {
  display: inline-block;
  margin-left: 17rpx;
  font-size: 48rpx;
}
.userMesseBox .userMesseContant .userMesseContant_right .userMesseContant_rightRecharge {
  margin-top: 35rpx;
  color: #ffd270;
  font-size: 30rpx;
}
.tabBox {
  position: relative;
  border-radius: 75rpx 75rpx 0 0;
  transform: translateY(-60rpx);
  background-color: #fff;
  box-sizing: border-box;
  padding: 40rpx 60rpx;
}
.navbar {
  display: flex;
  justify-content: space-between;
  box-sizing: border-box;
  height: 75rpx;
}
.navbar .item {
  flex: .33;
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
  text-align: center;
  line-height: 60rpx;
  font-size: 30rpx;
  color: #a5a5a5;
  font-family: Microsoft YaHei;
}
.navbar .item.active {
  color: #000;
  font-size: 32rpx;
  font-family: Microsoft YaHei;
}
.navbar .item.active:after {
  content: "";
  position: absolute;
  bottom: -5rpx;
  left: 0;
  right: 0;
  margin: auto;
  height: 8rpx;
  width: 65rpx;
  border-radius: 50rpx;
  background-color: #000;
}
.navbar_contantBox {
  margin-top: 30rpx;
}
.navbar_contantBox .rechargeRecord {
  display: flex;
  justify-content: space-between;
  height: 107rpx;
  line-height: 107rpx;
  border-bottom: 1px solid #f1f1f1;
}
.navbar_contantBox .rechargeRecord view:nth-child(1) {
  position: relative;
  font-size: 36rpx;
  color: #333;
}
.navbar_contantBox .rechargeRecord view:nth-child(1) view {
  position: absolute;
  width: 270rpx;
  left: 0;
  right: 0;
  margin: auto;
  bottom: 7rpx;
  font-size: 18rpx;
  line-height: 18rpx;
  text-align: left;
  padding-left: 40rpx;
  color: red;
}
.Subcard {
  margin: 10rpx 0;
  position: relative;
  display: flex;
}
.noOverdue {
  background-image: url('http://www.qumatou.com.cn/zheng/xcximage/cardNow.png');
  height: 170rpx;
  background-repeat: no-repeat;
  background-size: 100%;
}
.Subcard .Subcard_left {
  flex: .7;
  padding: 25rpx 30rpx 0 30rpx;
}
.Subcard .Subcard_left view:nth-child(1) {
  background: linear-gradient(to right, #54422d, #ab7143);
  -webkit-background-clip: text;
  color: transparent;
  font-size: 30rpx;
}
.Subcard .Subcard_left view:nth-child(2) {
  font-size: 21rpx;
  transform: translateY(50rpx);
}
.Subcard .Subcard_right {
  flex: .3;
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-top: 35rpx;
  transform: translateX(-10rpx);
}
.Subcard .Subcard_right view:nth-child(1) {
  background: linear-gradient(to right, #54422d, #ab7143);
  -webkit-background-clip: text;
  color: transparent;
  font-size: 27rpx;
}
.Subcard .Subcard_right view:nth-child(2) {
  background: linear-gradient(to right, #54422d, #ab7143);
  -webkit-background-clip: text;
  color: transparent;
  font-size: 36rpx;
}
.overdue .Subcard_left view,
.overdue .Subcard_right view {
  color: #fff !important;
}
.overdue .Subcard_left .cardChapter,
.overdue .Subcard_right .cardChapter {
  position: absolute;
  width: 130rpx;
  height: 130rpx;
  left: 0;
  right: 0;
  margin: auto;
  top: 22rpx;
}
.shareCardBox {
  box-sizing: border-box;
  padding: 20rpx;
  margin-bottom: 30rpx;
  height: 195rpx;
  box-shadow: 0 0 10rpx #f1f1f1;
}
.shareCardBox .shareCardBox_top {
  display: flex;
  align-items: center;
}
.shareCardBox .shareCardBox_top image {
  height: 92rpx;
  width: 92rpx;
  border-radius: 50%;
}
.shareCardBox .shareCardBox_top .shareCardBox_topRight {
  display: flex;
  align-items: center;
  padding-left: 20rpx;
  flex: 1;
  justify-content: space-between;
}
.shareCardBox .shareCardBox_top .shareCardBox_topRight view:nth-child(1) {
  font-size: 30rpx;
  color: #000;
}
.shareCardBox .shareCardBox_top .shareCardBox_topRight view:nth-child(2) {
  font-size: 24rpx;
}
.shareCardBox .shareCardBox_bottom {
  display: flex;
  align-items: center;
  padding-top: 15rpx;
}
.shareCardBox .shareCardBox_bottom view:nth-child(1) {
  flex: .7;
  padding-left: 10rpx;
  font-size: 30rpx;
}
.shareCardBox .shareCardBox_bottom view:nth-child(2) {
  flex: .3;
  text-align: center;
  font-size: 30rpx;
  color: red;
  border: 1rpx solid #f1f1f1;
}
.noStatus {
  font-size: 27rpx;
  text-align: center;
  padding-top: 245rpx;
}
