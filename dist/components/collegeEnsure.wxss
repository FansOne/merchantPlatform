.ensure_box {
  box-sizing: border-box;
  padding-top: 40rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.ensure_box .ensure {
  display: flex;
  flex: 1;
  justify-content: space-between;
  align-items: center;
}
.ensure_box .ensure .ensure_item {
  display: flex;
  align-items: center;
}
.ensure_box .ensure .ensure_item image {
  width: 35rpx;
  height: 35rpx;
  margin-right: 7rpx;
}
.ensure_box .ensure .ensure_item view {
  color: #805915;
  font-size: 26rpx;
}
