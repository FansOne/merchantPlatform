@import "./../components/payedCouponItem.wxss";
.backindex_box {
  z-index: 9;
  height: 75rpx;
  width: 76rpx;
  position: absolute;
  right: 10rpx;
  top: 100rpx;
  margin: auto;
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: rgba(0, 0, 0, 0.4);
  border-radius: 50rpx;
  padding-top: 2rpx;
}
.backindex_box image {
  height: 41rpx;
  width: 45rpx;
}
.backindex_box view {
  font-size: 21rpx;
  color: white;
}
.rgba_background {
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0);
  position: absolute;
  top: 0;
  left: 0;
  z-index: 100;
}
.paymentTop {
  height: 276rpx;
  padding-top: 45rpx;
  box-sizing: border-box;
  background-color: #ffd270;
  display: flex;
  flex-direction: column;
  color: #ffffff;
  font-size: 32rpx;
  font-weight: bold;
  letter-spacing: 3rpx;
}
.paymentTop image {
  width: 65rpx;
  height: 65rpx;
}
.paymentTop view {
  display: flex;
  justify-content: center;
}
.paymentTop text {
  display: block;
  text-align: center;
  margin-top: 15rpx;
}
.Mask {
  position: absolute;
  top: 0;
  left: 0;
  z-index: 9;
  background-color: rgba(0, 0, 0, 0.6);
  width: 100%;
}
page {
  width: 100%;
  height: 100%;
  background-color: #fff;
}
.container {
  width: 100%;
  height: 100%;
}
.nodata {
  text-align: center;
  margin-top: 550rpx;
}
.coupons_tab {
  display: inline-block;
  width: auto;
  font-size: 34rpx;
  line-height: 110rpx;
  margin-left: 30rpx;
  position: relative;
}
.coupons_selected::after {
  content: '';
  width: 40rpx;
  height: 6rpx;
  background-color: #ff5038;
  border-radius: 3rpx;
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
  bottom: 16rpx;
}
