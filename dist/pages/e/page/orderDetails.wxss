page {
  background-color: #ffffff;
  margin-bottom: 120rpx;
}
.deliverGoods-box {
  display: flex;
  flex-direction: row;
  height: 262rpx;
  width: 100%;
  box-sizing: border-box;
  padding: 52rpx 40rpx;
  background-color: #efeff4;
}
.deliverGoods-box .goodsStatus {
  width: 100%;
}
.deliverGoods-box .goodsStatus view {
  margin-top: 20rpx;
  line-height: 60rpx;
}
.deliverGoods-box .jiantou {
  display: flex;
  align-items: center;
  width: 20%;
  height: 264rpx;
  justify-content: center;
  transform: translateY(-52rpx);
}
.deliverGoods-box .jiantou image {
  width: 45rpx;
  height: 45rpx;
}
.deliverGoods-box .goodsStatus-paddingTop {
  padding-top: 23rpx;
}
.bottom-btnBox {
  position: fixed;
  bottom: 0;
  height: 120rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
  line-height: 120rpx;
  text-align: center;
}
.bottom-btnBox button:nth-child(1) {
  flex: 0.5;
  background-color: #343434;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 33rpx;
  color: #ffffff;
  letter-spacing: 3rpx;
  border-radius: 0;
}
.bottom-btnBox button:nth-child(2) {
  flex: 0.5;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 33rpx;
  background-color: #ef6e4b;
  color: #ffffff;
  letter-spacing: 3rpx;
  border-radius: 0;
}
.bottom-btnBox button.disabledButton {
  background-color: #b1b2b1;
}
.bottom-btnBox button.applySale {
  background-color: #ef6e4b;
}
.numberAndTime {
  margin: 0 40rpx;
  display: flex;
  border-bottom: 1px solid #efeff4;
  flex-direction: column;
}
.numberAndTime .copyNumber {
  margin-top: 25rpx;
  display: flex;
  justify-content: space-between;
  flex-direction: row;
}
.numberAndTime .copyNumber view:nth-child(1) {
  width: 30%;
  text-align: left;
}
.numberAndTime .copyNumber .copyContentBox {
  display: flex;
  flex-direction: row;
  color: #969696;
}
.tblin_items_btn {
  border: 1px solid #969696;
  font-size: 23rpx;
  display: flex;
  color: #969696;
  align-items: center;
  justify-content: center;
  width: 95rpx;
  margin-right: 5rpx;
  margin-left: 20rpx;
}
.orderTime_box {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  margin: 25rpx 0;
}
.orderTime_box .order_time {
  color: #969696;
  font-size: 33rpx;
  letter-spacing: 2rpx;
}
.depressedIcon {
  /*width: 500rpx;*/
  transform: translateY(10rpx);
  position: absolute;
  left: 50%;
  padding-top: 20rpx;
}
.depressedIcon image {
  width: 88rpx;
  height: 88rpx;
}
