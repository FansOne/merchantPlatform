@import "./../../../../components/moreFormId.wxss";
page {
  position: relative;
  background-color: #e5e5e5;
}
.bgBannerBox {
  width: 100%;
  box-sizing: border-box;
  background-color: #fff;
  margin-top: 30rpx;
  padding: 0 30rpx;
}
.bgBannerBox .addressMsgBox {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.bgBannerBox .addressMsgBox .saLeft {
  flex: 1;
  display: flex;
  flex-direction: column;
}
.bgBannerBox .addressMsgBox .saLeft .userPhoneNum,
.bgBannerBox .addressMsgBox .saLeft .userAddress_ {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx 0;
}
.bgBannerBox .addressMsgBox .saLeft .userPhoneNum view:first-of-type,
.bgBannerBox .addressMsgBox .saLeft .userAddress_ view:first-of-type {
  color: #888;
  font-size: 26rpx;
}
.bgBannerBox .addressMsgBox .saLeft .userPhoneNum view:last-of-type,
.bgBannerBox .addressMsgBox .saLeft .userAddress_ view:last-of-type {
  flex: 1;
  font-size: 26rpx;
  text-align: left;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
.bgBannerBox .addressMsgBox .saLeft .userPhoneNum image,
.bgBannerBox .addressMsgBox .saLeft .userAddress_ image {
  width: 40rpx;
  height: 40rpx;
}
.bgBannerBox .addressMsgBox .saLeft .userPhoneNum {
  border-bottom: 1rpx dashed #e0e0e0;
}
.bgBannerBox .addressMsgBox .saRight {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
  padding-left: 35rpx;
}
.bgBannerBox .addressMsgBox .saRight view {
  font-size: 24rpx;
  background-color: #ecb1a5;
  padding: 0 10rpx;
  border-radius: 10rpx;
}
.bgBannerBox .addressMsgBox .saRight image {
  width: 60rpx;
  height: 60rpx;
  margin-top: 26rpx;
}
.featuredItem {
  margin-top: -15rpx;
}
.threeIcon_box {
  background-color: #fff;
  box-sizing: border-box;
  width: 100%;
  margin-top: 30rpx;
  margin-bottom: 30rpx;
}
.threeIcon_box .threeIcon_flex {
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: space-around;
}
.threeIcon_box .threeIcon_flex .threeIcon_item {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 150rpx;
  height: 169rpx;
  font-size: 28rpx;
  color: #333;
}
.threeIcon_box .threeIcon_flex .threeIcon_item image {
  margin-top: 20rpx;
  width: 82rpx;
  height: 82rpx;
}
.threeIcon_box .threeIcon_flex .threeIcon_item view {
  color: #333;
  font-size: 25rpx;
  margin-top: 13rpx;
}
.threeIcon_box .threeIcon_flex .threeIcon_itemnh {
  height: 150rpx;
  display: flex;
  align-items: center;
}
.threeIcon_box .threeIcon_flex .threeIcon_itemnh image {
  height: 85rpx;
  width: 100%;
}
.indexGoods_box .indexGoods_item {
  margin-top: 30rpx;
  display: flex;
}
.indexGoods_box .indexGoods_item image {
  width: 240rpx;
  height: 240rpx;
  border-radius: 7rpx;
}
.indexGoods_box .indexGoods_item .indexGoods_right {
  flex: 1;
  padding-left: 25rpx;
}
.indexGoods_box .indexGoods_item .indexGoods_right .indexGoods_title {
  color: #000;
  font-size: 33rpx;
  width: 448rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.indexGoods_box .indexGoods_item .indexGoods_right .indexGoods_subtitle {
  margin: 28rpx 0;
  font-size: 22rpx;
  width: 448rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.indexGoods_box .indexGoods_item .indexGoods_right .indexGoods_new {
  width: 66rpx;
  height: 36rpx;
  margin-bottom: 28rpx;
  background-color: #fc8161;
  font-size: 20rpx;
  color: #fff;
  line-height: 36rpx;
  text-align: center;
  letter-spacing: 3rpx;
  border-radius: 7rpx;
}
.indexGoods_box .indexGoods_item .indexGoods_right .indexGoods_price {
  display: flex;
}
.indexGoods_box .indexGoods_item .indexGoods_right .indexGoods_price .indexGoods_nowPrice {
  border: 2px solid red;
  width: 64rpx;
  height: 36rpx;
  font-size: 22rpx;
  line-height: 36rpx;
  text-align: center;
  color: red;
}
.indexGoods_box .indexGoods_item .indexGoods_right .indexGoods_price .indexGoods_nowPrice_right {
  margin-left: 15rpx;
  color: red;
}
.indexGoods_box .indexGoods_item .indexGoods_right .indexGoods_price .indexGoods_nowPrice_right .nowPrice {
  font-size: 36rpx;
}
.indexGoods_box .indexGoods_item .indexGoods_right .indexGoods_price .indexGoods_nowPrice_right .originalPrice {
  font-size: 25rpx;
  text-decoration: line-through;
}
.content-wrap {
  width: 720rpx;
  height: auto;
  border-radius: 6rpx;
  margin: 0rpx auto;
  padding-bottom: 20rpx;
  box-sizing: border-box;
}
.content-wrap .shop-wrap-title {
  font-size: 32rpx;
  color: #000;
  text-align: center;
  padding: 15rpx 0 10rpx 0;
}
.content-wrap .shop-wrap {
  width: 100%;
  height: auto;
  margin: 10rpx auto 0;
}
.content-wrap .shop-wrap image {
  display: block;
}
.swiperShare {
  position: fixed;
  top: 200rpx;
}
